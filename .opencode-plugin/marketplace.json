      "source": "./plugins/framework-specialists",
      "description": "Framework experts: React, Vue, Angular, Next.js, Django, FastAPI",
      "version": "1.30.0",
      "author": "OpenCodeAutoPM Team",
      "category": "Frameworks",
      "keywords": ["react", "vue", "angular", "nextjs", "django", "fastapi"],
      "agents": [
        "@react-specialist",
        "@vue-specialist",
        "@angular-specialist",
        "@nextjs-expert",
        "@django-expert",
        "@fastapi-expert"
      ],
      "commands": [
        "/framework:scaffold",
        "/framework:optimize"
      ],
      "hooks": [],
      "mcpServers": []
