        "core-agents",
        "devops-suite",
        "cloud-architects",
        "database-experts",
        "testing-framework",
        "pm-commands",
        "azure-devops",
        "github-automation",
        "language-specialists",
        "framework-specialists",
        "data-engineering",
        "security-auditing"
      ]
